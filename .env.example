# DeepSeek API Configuration
# 从 https://platform.deepseek.com/ 获取你的 API Key

# DeepSeek API Key (必填)
VITE_DEEPSEEK_API_KEY=your_deepseek_api_key_here

# DeepSeek API URL (可选，默认使用官方地址)
VITE_DEEPSEEK_API_URL=https://api.deepseek.com/v1/chat/completions

# DeepSeek 模型名称 (可选，默认使用 deepseek-chat)
VITE_DEEPSEEK_MODEL=deepseek-chat

# Gemini API Configuration (用于文档解析和OCR)
# 从 https://aistudio.google.com/app/apikey 获取你的 API Key
# 或使用中转 API

# Gemini API Key (可选，用于文档解析功能)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Gemini API URL (可选)
# 官方地址: https://generativelanguage.googleapis.com/v1beta
# 中转地址示例: https://your-proxy-api.com/v1beta
VITE_GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta

# Gemini 模型名称 (可选，默认使用 gemini-2.0-flash)
VITE_GEMINI_MODEL=gemini-2.0-flash

# 是否启用Gemini文档解析 (可选，默认为 true)
# 如果不需要文档上传功能，设置为 false
VITE_ENABLE_GEMINI_DOCUMENT_PROCESSING=true

# Gemini API Configuration (用于文档解析和OCR)
# 从 https://aistudio.google.com/app/apikey 获取你的 API Key

# Gemini API Key (可选，用��文档解析)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Gemini API URL (可选，默认使用官方地址)
VITE_GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta

# Gemini 模型名称 (可选，默认使用 gemini-2.0-flash)
VITE_GEMINI_MODEL=gemini-2.0-flash

# 是否启用Gemini文档解析 (可选，默认为 true)
VITE_ENABLE_GEMINI_DOCUMENT_PROCESSING=true
